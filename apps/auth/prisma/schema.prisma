generator client {
  provider = "prisma-client-js"
  output   = "../../../node_modules/@prisma/client-auth"
  binaryTargets = ["darwin-arm64", "linux-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int      @id @default(autoincrement())
  email        String   @unique
  passwordHash String
  createdAt    DateTime @default(now())
}
